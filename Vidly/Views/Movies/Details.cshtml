@model Vidly.Models.Movie
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>@Model.Genre</li>
    <li>@Model.ReleaseDate.ToLongDateString()</li>
    <li>@Model.DateAdded.ToLongDateString()</li>
    <li>@Model.NumberInStock</li>
</ul>

